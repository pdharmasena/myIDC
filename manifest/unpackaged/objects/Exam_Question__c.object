    <validationRules>
        <fullName>Check_for_Answer</fullName>
        <active>true</active>
        <description>Does the question have an answer</description>
        <errorConditionFormula>ISBLANK( TEXT( Correct_Answer__c ) )</errorConditionFormula>
        <errorDisplayField>Correct_Answer__c</errorDisplayField>
        <errorMessage>You must set an &quot;Answer&quot; for this question!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_have_a_Section</fullName>
        <active>true</active>
        <description>Exam question must have a section entered</description>
        <errorConditionFormula>AND(
ISBLANK( TEXT(Exam_Section__c) ), IF(Exam_Type__c = &quot;Theory&quot;, true, false)
)</errorConditionFormula>
        <errorDisplayField>Exam_Section__c</errorDisplayField>
        <errorMessage>You must set a exam section (e.g. Physics, Physiology, etc.) for this exam question</errorMessage>
    </validationRules>
